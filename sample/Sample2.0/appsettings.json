{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },

  "Authentication": {
    "Authority": "https://sample.auth0.com",
    "ResponseType": "code",
    "ClientId": "USER_SECRET",
    "ClientSecret": "USER_SECRET",
    "Scope": [ "openid", "profile", "email" ],
    "CallbackPath": "/signin-auth0",
    "ClaimsIssuer": "Auth0"
  }
}
